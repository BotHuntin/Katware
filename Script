local function OWBWWR_fake_script() -- RainbowFrame.LocalScript 
	local script = Instance.new('LocalScript', RainbowFrame)

	local t = 10;
	
	while wait() do
	
		local hue = tick() % t / t
	
		local color = Color3.fromHSV(hue,1,1)
	
		script.Parent.BackgroundColor3 = color
	
	end
end
