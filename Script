	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.AimbotTab.Visible = false
		script.Parent.Parent.VisualsTab.Visible = false
		script.Parent.Parent.CreditsTab.Visible = true
		script.Parent.Parent.MiscTab.Visible = false
	end)
end
coroutine.wrap(BVREUX_fake_script)()
local function WHUWS_fake_script() -- Miscellaneous.LocalScript 
	local script = Instance.new('LocalScript', Miscellaneous)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.AimbotTab.Visible = false
		script.Parent.Parent.VisualsTab.Visible = false
		script.Parent.Parent.CreditsTab.Visible = false
		script.Parent.Parent.MiscTab.Visible = true
	end)
end
coroutine.wrap(WHUWS_fake_script)()
local function PMDOC_fake_script() -- RainbowFrame.LocalScript 
	local script = Instance.new('LocalScript', RainbowFrame)

	local t = 10;
	
	while wait() do
	
		local hue = tick() % t / t
	
		local color = Color3.fromHSV(hue,1,1)
	
		script.Parent.BackgroundColor3 = color
	
	end
end
coroutine.wrap(PMDOC_fake_script)()
local function CMGEXB_fake_script() -- ScreenGui.Keybind 
	local script = Instance.new('LocalScript', ScreenGui)

	local Frame = script.Parent.Frame
	local Opened = false
	
	local PositionClosed = UDim2.new(-0.369, 0,0.327, 0)
	local PositionOpened = UDim2.new(0.182, 0,0.425, 0)
	
	local UserInputService = game:GetService("UserInputService")
	
	UserInputService.InputBegan:Connect(function(KeyCode)
		if KeyCode.KeyCode == Enum.KeyCode.Insert then
			if Opened then
				Frame:TweenPosition((PositionClosed), "InOut", "Sine")
				Opened = false
			else
				Frame:TweenPosition((PositionOpened), "InOut", "Sine")
				Opened = true
			end
		end
	end)
end
coroutine.wrap(CMGEXB_fake_script)()
