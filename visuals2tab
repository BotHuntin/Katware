-- Scripts:

local function CPNF_fake_script() -- rainbowFrame2.LocalScript 
	local script = Instance.new('LocalScript', rainbowFrame2)

	local t = 10;
	
	while wait() do
	
		local hue = tick() % t / t
	
		local color = Color3.fromHSV(hue,1,1)
	
		script.Parent.BackgroundColor3 = color
	
	end
end
coroutine.wrap(CPNF_fake_script)()
local function MMEKGM_fake_script() -- rainbowFrame1.LocalScript 
	local script = Instance.new('LocalScript', rainbowFrame1)

	local t = 10;
	
	while wait() do
	
		local hue = tick() % t / t
	
		local color = Color3.fromHSV(hue,1,1)
	
		script.Parent.BackgroundColor3 = color
	
	end
end
coroutine.wrap(MMEKGM_fake_script)()
