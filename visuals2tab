	for _,i in pairs(workspace:GetDescendants()) do
		if i:IsA("BasePart") and not i.Parent:FindFirstChildOfClass('Humanoid') and not i.Parent.Parent:FindFirstChildOfClass('Humanoid') then
			i.Material = "ForceField"
		end
	end
end)

